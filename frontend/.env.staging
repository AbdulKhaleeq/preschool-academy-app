# STAGING Frontend Configuration

# API Base URL - Will be your Railway backend URL
REACT_APP_API_URL=https://your-backend-staging.railway.app

# App Configuration
REACT_APP_NAME=Preschool Academy (Staging)
REACT_APP_VERSION=1.0.0-staging

# Environment
REACT_APP_ENV=staging

# Feature Flags for staging
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_CONSOLE_LOGS=false

# Staging specific settings
REACT_APP_STAGING_MODE=true

# Analytics (disabled for staging)
REACT_APP_GA_TRACKING_ID=

# Error Tracking (optional for staging)
REACT_APP_SENTRY_DSN=